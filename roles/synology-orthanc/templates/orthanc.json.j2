{
  "Name" : "Orthanc {{ orthanc_name }} on Synology",
  "StorageDirectory" : "/var/lib/orthanc/storage",
  "IndexDirectory" : "/var/lib/orthanc/storage",

  "PostgreSQL" : {
    "EnableIndex" : true,
    "EnableStorage" : false,
    "Host" : "{{ orthanc_name }}-postgres",
