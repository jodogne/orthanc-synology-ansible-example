  "HttpsCACertificates" : "",
  "UserMetadata" : {  },
  "UserContentType" : {  },
  "StableAge" : 60,
  "StrictAetComparison" : false,
  "StoreMD5ForAttachments" : true,
  "LimitFindResults" : 200,
  "LimitFindInstances" : 500,
  "LimitJobs" : 10,
  "LogExportedResources" : true,
  "KeepAlive" : false,
  "StoreDicom" : true,
  "DicomAssociationCloseDelay" : 5,
  "QueryRetrieveSize" : 10,
